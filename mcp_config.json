            "command": "python",
            "args": [
                "./mcp_server/mcp_server_weather.py"
            ],
            "transport": "stdio"
        },
        "file_manager": {
            "command": "python",
            "args": [
                "./mcp_server/mcp_server_file_manager.py"
            ],
            "transport": "stdio"
